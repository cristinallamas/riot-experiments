{
  "name": "Feed.js",
  "description": "Riot Feed",
  "version": "0.1.0",
  "author": "Cristina Llamas <cristina_roman@dennis.co.uk>",
  "private": true,
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules"
  ],
  "devDependencies": {
    "riot": "^2.2.4",
    "superstatic": "4.0.2",
    "riot-compiler": "3.2.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "dependencies": {
    "browserify": "^14.3.0",
    "twitter": "^1.7.0"
  },
  "scripts": {
    "watch-css": "catw -c 'lessc -' 'style/*.less' -o static/bundle.css -v",
    "watch-js": "watchify browser/*.js -o static/bundle.js -dv",
    "watch": "npm run watch-css & npm run watch-js",
    "build-css": "catw -c 'lessc -' 'style/*.less' > static/bundle.css",
    "build-js": "./node_modules/.bin/browserify src/*.js > dist/bundle.js",
    "build": "npm run build-css && npm run build-js"
  }
}
